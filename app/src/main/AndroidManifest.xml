<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.overswayit.plesnisavezsrbije">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:name=".MyApp"
        android:allowBackup="false"
        tools:replace="android:allowBackup"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.CoupleActivity"></activity>
        <activity android:name=".activities.RatingListActivity" />
        <activity android:name=".activities.PointListActivity" />
        <activity android:name=".activities.ListActivity" />
        <activity
            android:name=".MainActivity"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ClubsActivity"
            android:label="@string/clubs"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activities.AdjudicatorsActivity"
            android:label="@string/adjudicators"
            android:theme="@style/AppTheme" />
        <activity
            android:name=".activities.ClubActivity"
            android:label="@string/clubs"
            android:theme="@style/AppTheme" />
    </application>

</manifest>