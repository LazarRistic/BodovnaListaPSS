        <attr name="danceCategoryLatin" />
        <attr name="danceCategoryStandard" />
        <attr name="danceCategoryCombined" format="string" />
        <attr name="danceCategoryColorLatin" />
        <attr name="danceCategoryColorStandard" />
        <attr name="danceCategoryColorCombined" format="color" />
    </declare-styleable>
